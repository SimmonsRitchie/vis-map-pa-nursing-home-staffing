{
  "extends": "airbnb",
  "root": true,
  "parser": "babel-eslint",
  "rules": {
    "import/no-extraneous-dependencies": "off",
    "import/no-unresolved": "off",
    "no-console": "off",
    "react/jsx-filename-extension": "off",
    "no-undef": "off",
    "react/prop-types": "on",
    "class-methods-use-this": "off",
    "no-param-reassign": "off",
    "implicit-arrow-linebreak": "off",
    "max-len": "off",
    "no-underscore-dangle": "off",
    "camelcase": "off"
  }
}
